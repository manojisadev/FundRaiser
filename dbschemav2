CREATE TABLE Team_Members (
uid INT NOT NULL,
pid INT NOT NULL,
PRIMARY KEY (uid,pid),
FOREIGN KEY (uid) REFERENCES Users(uid),
FOREIGN KEY (pid) REFERENCES Projects(pid)
);



INSERT INTO Team_Members (uid,pid) VALUES (1,11);
