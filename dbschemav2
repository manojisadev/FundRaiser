CREATE TABLE Users (
uid INT NOT NULL,
Ufirst_name VARCHAR(45) NOT NULL,
Uaddress VARCHAR(45) NULL,
Uemail VARCHAR(45) NOT NULL UNIQUE,
Ulast_name VARCHAR(45) NOT NULL,
Upassword VARCHAR(45) NOT NULL,
Uphone INT(10) NULL,
Ujoin_date DATETIME NOT NULL,
PRIMARY KEY (uid)
);

