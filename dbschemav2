CREATE TABLE Comments (
pid INT NOT NULL,
uid INT NOT NULL,
Ccomment VARCHAR(255) NOT NULL,
Ccomment_time DATETIME NOT NULL,
PRIMARY KEY (pid,uid,Ccomment_time),
FOREIGN KEY (uid) REFERENCES Users(uid),
FOREIGN KEY (pid) REFERENCES Projects(pid)
);

