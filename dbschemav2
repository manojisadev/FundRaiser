CREATE TABLE CreditCard (
uid INT NOT NULL,
CNID INT NOT NULL,
CreditNumber BIGINT(16) NOT NULL,
CVVNumber INT(3) NOT NULL,
ExpirationDate DATE NOT NULL,
OwnerName VARCHAR(45) NOT NULL,
PRIMARY KEY (uid, CNID),
FOREIGN KEY (uid) REFERENCES Users(uid)
);

