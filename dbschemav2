CREATE TABLE Ratings (
uid INT NOT NULL,
pid INT NOT NULL,
Rrating INT NOT NULL,
PRIMARY KEY (uid,pid,Rrating),
FOREIGN KEY (uid,pid) REFERENCES Donors(uid,pid)
);

