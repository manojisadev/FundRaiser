CREATE TABLE Follows (
uid INT NOT NULL,
fid INT NOT NULL,
PRIMARY KEY (uid,fid),
FOREIGN KEY (uid) REFERENCES Users(uid),
FOREIGN KEY (fid) REFERENCES Users(uid)
);

