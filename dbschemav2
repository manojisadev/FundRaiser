CREATE TABLE Likes (
pid INT NOT NULL,
uid INT NOT NULL,
Lliked_time DATETIME NOT NULL,
PRIMARY KEY (pid,uid),
FOREIGN KEY (uid) REFERENCES Users(uid),
FOREIGN KEY (pid) REFERENCES Projects(pid)
);

